{
  "name": "ikatoo-backend",
  "author": {
    "name": "Milton Carlos Katoo",
    "email": "mckatoo@gmail.com",
    "url": "https://www.linkedin.com/in/mckatoo/"
  },
  "version": "1.0.0",
  "main": "index.js",
  "license": "CC0-1.0",
  "scripts": {
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "test": "NODE_ENV=test jest",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "express": "ts-node src/infra/http/express/index.ts",
    "dev": "NODE_ENV=test tsnd -r tsconfig-paths/register --watch src/ --respawn --transpile-only src/infra/http/express/index.ts",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:express": "NODE_ENV=test jest src/infra/http/express",
    "test:express:watch": "NODE_ENV=test jest src/infra/http/express --watch"
  },
  "devDependencies": {
    "@swc/core": "1.2.203",
    "@swc/jest": "0.2.21",
    "@types/express": "4.17.13",
    "@types/jest": "28.1.2",
    "@types/jsrsasign": "10.5.2",
    "@types/node": "18.0.0",
    "@types/supertest": "2.0.12",
    "cross-env": "7.0.3",
    "import-sort-cli": "6.0.0",
    "import-sort-parser-babylon": "6.0.0",
    "import-sort-parser-typescript": "6.0.0",
    "import-sort-style-renke": "6.0.0",
    "jest": "28.1.1",
    "supertest": "6.2.3",
    "ts-node": "10.8.1",
    "ts-node-dev": "2.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "4.7.3"
  },
  "dependencies": {
    "dotenv": "16.0.1",
    "express": "4.18.1",
    "jsrsasign": "10.5.27"
  },
  "importSort": {
    ".js, .jsx, .es6, .es": {
      "parser": "babylon",
      "style": "renke"
    },
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "renke"
    }
  }
}
